{
    "name": "Mars Launcher",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "A Chromium extension to open mars link with option to add params.",
    "icons": {
        "16": "icons/logo16x16.png",
        "32": "icons/logo32x32.png",
        "48": "icons/logo48x48.png",
        "128": "icons/logo128x128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "*://*.568win.com/*",
        "*://*.568winex.com/*",
        "*://*.ttbbyyllyy.com/*",
        "storage"
    ]
}